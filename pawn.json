{
    "user": "Skisowisko",
    "repo": "skis",
    "include_path": "src",
    "resources": [
        {
            "name": "^pawnraknet-(.*)-linux.tar.gz$",
            "platform": "linux",
            "archive": true,
            "plugins": ["skis.so"]
        },
        {
            "name": "^pawnraknet-(.*)-win32.zip$",
            "platform": "windows",
            "archive": true,
            "plugins": ["skis.dll"]
        }
    ],
    "runtime": {
        "plugins": ["Skisowisko/skis"]
    }
}
